{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "node12",
      "entrypoint": "./index.js",
      "events": [{ "websocket": "" }]
    }
  }
}
